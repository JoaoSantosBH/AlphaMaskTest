<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/card"
        android:drawable="@drawable/illustration" />
    <item
        android:id="@+id/shape">
        <rotate
            android:fromDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270"
            android:drawable="@drawable/ic_shape" />
    </item>
</layer-list>